{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "crons": [
      {
        "path": "/api/demo-data/seed-workouts",
        "schedule": "0 9 * * *",
        "headers": {
          "Authorization": "Bearer $CRON_SECRET"
        }
      }
    ]
  }